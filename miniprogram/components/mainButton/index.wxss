/* components/mainButton.wxss */
.mainButton {
  width: 184px;
  height: 40px;
  line-height: 40px;
  text-align: center;
  background-color: #4D50A4;
  border-radius: 8px;
  color: #FFFFFF;
}

.mainButtonDisabled {
  width: 184px;
  height: 40px;
  line-height: 40px;
  text-align: center;
  background-color: #b9b9b9;
  border-radius: 8px;
  color: #2b2b2b;
}

.secondaryButton {
  width: 184px;
  height: 40px;
  line-height: 40px;
  text-align: center;
  color: #4D50A4;
  border: 1px solid #4D50A4;
  border-radius: 8px;
}

.recordButton-fill {
  font-size: 14px !important;
  line-height: 28px;
  width: 66px !important;
  height: 28px !important;
  color: white;
  background-color: #4D50A4;
  border-radius: 1020px;
  position: relative;
}
.recordButton-empty {
  font-size: 14px !important;
  line-height: 28px;
  width: 66px !important;
  height: 28px !important;
  color: #4D50A4;
  background: #FFFFFF;
  border: 1px solid #4D50A4;
  border-radius: 1020px;
  position: relative;
}
.analyst-fill {
  font-size: 14px !important;
  line-height: 28px;
  width: 66px !important;
  height: 28px !important;
  color: white;
  background-color: #4D50A4;
  border-radius: 1020px;
  position: relative;
}
.analyst-empty {
  font-size: 14px !important;
  line-height: 28px;
  width: 66px !important;
  height: 28px !important;
  color: #4D50A4;
  background: #FFFFFF;
  border: 1px solid #4D50A4;
  border-radius: 1020px;
  position: relative;
}

.button_icon {
  position: absolute;
  left: 10px;
  top: 5px;
  width: 16px;
  height: 17px;
}
.record_title {
  position: absolute;
  left: 28px;
}
.help-fill {
  font-size: 14px !important;
  line-height: 28px;
  text-align: center;
  width: 72px !important;
  height: 28px !important;
  color: white;
  background-color: #4D50A4;
  border-radius: 1020px;
}
.help-empty {
  font-size: 14px !important;
  line-height: 28px;
  text-align: center;
  width: 72px !important;
  height: 28px !important;
  color: #4D50A4;
  border: 1px solid #4D50A4;
  border-radius: 1020px;
}

.oneMonth-empty, .threeMonth-empty, .sixMonth-empty {
  font-size: 16px !important;
  line-height: 28px;
  width: 50px !important;
  height: 28px !important;
  color: #4D50A4;
  background: #FFFFFF;
   /* border: 1px solid #4D50A4; */
  opacity: 50%;
  border-radius: 1020px;
  position: relative;
  margin-right: 5px;
}

.oneMonth-empty text, .threeMonth-empty text, .sixMonth-empty text, .podcast-empty text, .meditation-empty text {
  position: relative;
  left: 5.5px;
}

.oneMonth-fill, .threeMonth-fill, .sixMonth-fill {
  font-size: 16px !important;
  line-height: 28px;
  width: 50px !important;
  height: 28px !important;
  color: #4D50A4;
  background: #FFFFFF;
  border: 1px solid #4D50A4;
  border-radius: 1020px;
  position: relative;
  margin-right: 5px;
  bottom: 2px;
}

.oneMonth-fill text, .threeMonth-fill text, .sixMonth-fill text, .podcast-fill text, .meditation-fill text {
  position: relative;
  left: 5.5px;
}

 .podcast-empty, .meditation-empty {
  font-size: 30rpx;
  line-height: 55rpx;
  color: #4D50A4;
  background: #FFFFFF;
  opacity: 100%;
  border: 1px solid #4D50A4;
  border-radius: 1020px;
  position: relative;
  margin-right: 30rpx;
  bottom: 2rpx;
}

.podcast-fill, .meditation-fill {
  font-size: 30rpx;
  line-height: 55rpx;
  /* height: 28px !important; */
  color:#FFFFFF;
  background: #4D50A4;
  border: 1px solid #4D50A4;
  border-radius: 1020px;
  position: relative;
  margin-right: 30rpx;
  bottom: 2rpx;
}

.podcast-fill, .podcast-empty {
  width: 225rpx;
}

.meditation-empty, .meditation-fill {
  width: 134rpx;
}

.type-fill {
  font-size: 30rpx;
  line-height: 55rpx;
  color:#FFFFFF;
  background: #4D50A4;
  border: 1px solid #4D50A4;
  border-radius: 1020px;
  position: relative;
  bottom: 2rpx;
  padding-left: 12rpx;
  padding-right: 12rpx;
}

.type-empty {
