.moledule {
  background-color: rgb(250, 250, 250);
  position: relative;
  display: flex;
  justify-content: space-between;
  margin-top: 3.0rem;
  margin-bottom: 2.5rem;
  border-radius: .5rem;
  width: 90%;
  height: 10rem;
  z-index: -1;
}

.content {
  position: relative;
  width: 96%;
  height: 10rem;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  z-index: 1;
}

.check-circle {
  width: 20px;
  height: 20px;
  border-radius: 50%;
  border-style: solid;
  border-width: 2px;
  border-color: black;
  margin-bottom: 70px;
}

.check-circle, text, .length {
  margin-top: 10px;
  margin-left: 10px;
}

.background {
  position: absolute;
  top: 0;
  left: 2%;
  width: 97%;
  height: auto;
}

#moledule-three {
  background-color: #BDD6FF;
}

#moledule-three > .content {
  height: 98%;
  flex-direction: column;
}