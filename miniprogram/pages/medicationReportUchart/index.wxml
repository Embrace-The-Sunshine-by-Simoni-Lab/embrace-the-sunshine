<view class="medicineContainer">
  <!-- **** purple square on top **** -->
  <view class="infoBox">
    <text class="medicineRecord">{{timePeriod}}本周情绪记录</text>
    <view class="medicineRecord_date">02月20日-02月27日</view>
  </view>
  <!-- line graph -->
  <view class="line_chart_container">
    <view class="line_chart_bar_container">
      <text class="line_chart_bar_container_title">自测抑郁症风险等级</text>
      <!-- box that contains the y label and line chart -->
      <view class="line_chart_body">
        <view class="line_chart_y_axis">
          <text class="line_chart_y_axis_label">重度</text>
          <text class="line_chart_y_axis_label">高度</text>
          <text class="line_chart_y_axis_label">中度</text>
          <text class="line_chart_y_axis_label">轻度</text>
          <text class="line_chart_y_axis_label">最低</text>
        </view>
        <!-- line chart 图片 -->
        <view class="charts-box">
          <view class="charts">
            <canvas canvas-id="jkyWEuYZpJWLcfbnKkmySDRjQLEpHsIG" id="jkyWEuYZpJWLcfbnKkmySDRjQLEpHsIG" class="charts" bindtouchstart="touchstart" bindtouchmove="touchmove" bindtouchend="touchend"/>
          </view>
        </view>
      </view>
    </view>
  </view>
  <view class="calendar_circle_container">
      <view class="calendar_circle">
        <l-circle percent='100' show-info='{{true}}' value-size='30' outer-diameter='310' inner-diameter='235' inner-color='#FFFFFF' 
        active-color="#91D300"
        ></l-circle>
        <view class="calendar_circle_text_container">
          <view class="calendar_circlel_text">12分</view>
        </view>
      </view>
      <view class="calendar_summary_container">
        <view class="calendar_summary_container_circle"></view>
        <text class="calendar_summary_container_text">轻度风险</text>
      </view>
  </view> 
</view>

